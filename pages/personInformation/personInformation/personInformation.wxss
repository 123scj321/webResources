/* pages/personInformation/personInformation/personInformation.wxss */
page{
  background-color: #f5f1ea;
  font-size:"黑体";
}
.myInformationContainer{
  padding:0;
  margin:0 auto;
}
.myInformationRow{
  width:100%;
  height:100rpx;
  overflow: hidden;
  line-height: 100rpx;
  background-color: #ffffff;
  margin-bottom: 10rpx;
  }
  .myInformationRowContext{
  margin:0 auto;
  width:90%;
  display:flex;
  flex-flow: row;
  justify-content: space-between;
  }
  .icon{
    width:10%;
    height:50rpx;
    margin-top:10rpx;
    margin-left:20rpx;
  }
  .icon>image{
    width:46rpx;
    height:46rpx;
  }
  .text{
    text-align: left;
    width:80%;
    color:#979797;
    font-size:28rpx;
  }
  .skip{
    width:5%;
    height:50rpx;
  }
  .skip>image{
    width:18rpx;
    height:26rpx;
  }